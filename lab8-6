#include <iostream>
using namespace std;
int main()
{
    int a,a1,k;
        cin>>a;
    a1=a;
    k=0;
    while (a1>0)
    {
        a1=a1/10;
        k=k+1;
    }
    if (a%2==0)
    {
        cout<<"Чётное ";
    }
    else
    {
        cout<<"Нечётное ";
    }
    if (k==1)
    {
        cout<<"однозначное ";
    }
    else if (k==2)
    {
        cout<<"двузначное ";
    }
    else if (k==3)
    {
        cout<<"трёхначное ";
    }
    cout<<"число";
    return 0;
}
