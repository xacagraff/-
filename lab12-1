#include <iostream>
using namespace std;
float PowerA3(float a)
{
    float b;
    b=a*a*a;
    return b;
}
int main()
{
    cout<<"Vvedite znachenia"<<endl;
    float x[5];
    for (int i=0; i<5; i++)
    {
        cin>>x[i];
    }
    for (int i=0; i<5; i++)
    {
        cout<<PowerA3(x[i])<<endl;
    }
}
