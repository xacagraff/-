#include <iostream>
using namespace std;
int main()
{
    cout<<"Vvedite chisla"<<endl;
    int a,b,c,k,nez;
    cin>>a>>b>>c; //A div C - количество квадратов, размещенных на стороне A,
                  //B div C - количество квадратов, размещенных на стороне B,
                  //Тогда общее количество квадратов равно произведению:
    k=(a/c)*(b/c);
    nez=(a*b)-k*c*c;
    cout<<"kol-vo kvadratov="<<k<<", ostatok="<<nez<<endl;
}
