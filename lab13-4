#include <iostream>
#include <math.h>
using namespace std;
int main()
{
  cout<<"Vvedite N"<<endl;
  int n;
  cin>>n;
  float a[n];
  cout<<"Vvedite elemeti massiva"<<endl;
  for (int i=0; i<n; i++)
  {
    cin>>a[i];
  }
  for (int i=0; i<n/2; i++)
  {
    cout<<a[i]<<endl;
    cout<<a[n-i-1]<<endl;
  }
  if (n%2==1)
  {
    cout<<a[n/2];
  }
  return 0;
}
