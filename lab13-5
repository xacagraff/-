#include <iostream>
#include <math.h>
using namespace std;
int main()
{
  cout<<"Vvedite N"<<endl;
  int n;
  cin>>n;
  float a[n];
  cout<<"Vvedite elemeti massiva"<<endl;
  for (int i=0; i<n; i++)
  {
    cin>>a[i];
  }
  for (int i=0; i<n; i++)
  {
    if (i%2==1)
    {
      cout<<a[i]<<endl;
    }
  }
  for (int i=n-1; i>=0; i--)
  {
    if (i%2==0)
    {
      cout<<a[i]<<endl;
    }
  }
  return 0;
}
