#include <iostream>
using namespace std;
int main()
{
    cout<<"Vvedite chisla"<<endl;
    int a,b,max;
    cin>>a>>b;
        if (a>b) {
            max=a;
        }
        else {
            max=b;
        }
    if (a==b) {
        a=0;
        b=0;
    }
    else {
        a=max;
        b=max;
    }
    cout<<"A="<<a<<", B="<<b<<endl;
}
