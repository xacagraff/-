#include <iostream>
using namespace std;
int main()
{
    int x;
    cin>>x;
    if (x>0) {
        if (x%2==0) {
            cout<<"Положительное чётное число"<<endl;
        }
        else { cout<<"Положительное нечётное число"<<endl; }
    }
    else if (x==0) { cout<<"Нулевое число"<<endl; }
    else { 
        if (x%2==0) {
            cout<<"Отрицательное чётное число"<<endl;
        }
        else { cout<<"Отрицательное нечётное число"<<endl; }
    }
    return 0;
}
