#include <iostream>
#include <math.h>
using namespace std;
int main()
{
  cout<<"Vvedite N"<<endl;
  int n;
  float a,b,sum;
  cin>>n;
  cout<<"Vvedite A and B"<<endl;
  cin>>a>>b;
  float arr[n];
  arr[0]=a;
  arr[1]=b;
  sum=a+b;
  for (int i=2; i<n; i++)
  {
    arr[i]=sum;
    sum+=arr[i];
    cout<<arr[i]<<endl;
  }
  return 0;
}
