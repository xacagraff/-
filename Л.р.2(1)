import math

x1 = int(input("Введите значчение по оси абсциссса: "))
y1 = int(input("Введите значение по ордите: "))
x2 = int(input("Введите значчение по оси абсциссса: "))
y2 = int(input("Введите значение по ордите: "))

print("Найдте расстояние между координатами")
S = math.sqrt( (x2 - x1)**2 + (y2-y1)**2 )

print("Расстояние между координатами равна", S)

print("Press enter to exit")
