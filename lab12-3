#include <iostream>
using namespace std;
float RignS(float r1,float r2)
{
    const float pi=3.14;
    return pi*r1*r1-pi*r2*r2;
}
int main()
{
    cout<<"Vvedite znachenia\n";
    float q1,q2,a1,a2,z1,z2;
    cin>>q1>>q2>>a1>>a2>>z1>>z2;
    cout<<"S1="<<RignS(q1,q2)<<endl;
    cout<<"S2="<<RignS(a1,a2)<<endl;
    cout<<"S3="<<RignS(z1,z2)<<endl;
    return 0;
}
