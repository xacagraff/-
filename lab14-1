#include <iostream>
using namespace std;
int main()
{
  cout<<"Vvedite N"<<endl;
  int n,k,l;
  cin>>n;
  float a[n];
  cout<<"Vvedite elemeti massiva"<<endl;
  for (int i=0; i<n; i++)
  {
    cin>>a[i];
  }
  cout<<"Vvedite K and L"<<endl;
  cin>>k>>l;
  float sum=0;
  for (int i=k; i<=l; i++)
  {
    sum+=a[i];
  }
  cout<<sum/(l-k+1);
  return 0;
}
