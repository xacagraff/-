#include <iostream>
using namespace std;
int main()
{
  int n;
  cout<<"Vvedite N"<<endl;
  cin>>n;
  int a[n];
  cout<<"Vvedite massiv"<<endl;
  for (int i=0; i<n; i++)
  {
    cin>>a[i];
  }
  int min=2147483647;
  for (int i=0; i<n; i++)
  {
    if ((i%2==0) and (a[i]<min))
    {
      min=a[i];
    }
  }
  cout<<"Minimum="<<min;
  return 0;
}
