#include <iostream>
using namespace std;
int Sign(float a)
{
    if (a==0)
    {
        return 0;
    }
    else if (a<0)
    {
        return -1;
    }
    else return 1;
}
int main()
{
    cout<<"Vvedite znachenia"<<endl;
    int a,b,x;
    cin>>a>>b;
    x=Sign(a)+Sign(b);
    cout<<x;
    return 0;
}
