#include <iostream>
using namespace std;
int Quarter(float x, float y)
{
    if (x>0 and y>0)
    {
        return 1;
    }
    if (x>0 and y<0)
    {
        return 4;
    }
    if (x<0 and y>0)
    {
        return 2;
    }
    if (x<0 and y<0)
    {
        return 3;
    }
}
int main()
{
    float q1,q2,a1,a2,z1,z2;
    cout<<"Vvedite znachenia"<<endl;
    cin>>q1>>q2>>a1>>a2>>z1>>z2;
    cout<<Quarter(q1,q2)<<endl;
    cout<<Quarter(a1,a2)<<endl;
    cout<<Quarter(z1,z2)<<endl;
}
